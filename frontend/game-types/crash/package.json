{
  "name": "crash-game-engine",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:ssl": "ng serve --configuration=ssl",
    "build": "ng build",
    "build:prod": "ng build --configuration prod",
    "build:stage": "ng build --configuration stage",
    "build:dev": "ng build --configuration dev",
